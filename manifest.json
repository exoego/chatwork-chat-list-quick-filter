{
  "name": "Chatwork Chat List Quick Filter",
  "description": "Filter chat list on chatwork.com",
  "manifest_version": 3,
  "version": "1.1.0",
  "homepage_url": "https://github.com/exoego/chatwork-chat-list-quick-filter",
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://www.chatwork.com/*"
      ],
      "js": [
        "src/index.js"
      ]
    }
  ],
  "applications": {
    "gecko": {
    }
  },
  "permissions": []
}
